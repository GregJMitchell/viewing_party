<center>
  <%= form_with model: current_user, url: "/users/#{current_user.id}", method: :patch, local: true do |f| %>

    <%= f.label :username %>
    <%= f.text_field :username, value: "#{current_user.username}" %><br>

    <%= f.label :email %>
    <%= f.text_field :email, value: "#{current_user.email}" %><br>

    <%= f.submit 'Update Profile' %>
  <% end %>
